{
  "expo": {
    "name": "BiteRight",
    "slug": "biterightapp",
    "scheme": "biteright-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/BiteRight_Icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/BiteRight_Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#129B68"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/BiteRight_Icon.png",
        "backgroundColor": "#129B68"
      },
      "package": "com.wrpl.biterightapp",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "splash": {
        "backgroundColor": "#129B68",
        "image": "./assets/images/BiteRight_Logo.png",
        "resizeMode": "contain"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/RobotoFlex-Regular.ttf"]
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "70d36a35-dc04-485d-8c5e-a513d0598804"
      }
    },
    "owner": "davinjason",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/70d36a35-dc04-485d-8c5e-a513d0598804"
    }
  }
}
